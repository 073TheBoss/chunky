<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Text?>

<StackPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Rectangle fill="#ddeef0" height="200.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="220.0">
         <effect>
            <DropShadow />
         </effect></Rectangle>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <bottom>
            <HBox alignment="CENTER_RIGHT" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="accept" defaultButton="true" mnemonicParsing="false" text="Accept" />
                  <Button fx:id="cancel" cancelButton="true" mnemonicParsing="false" text="Cancel" />
               </children>
            </HBox>
         </bottom>
         <center>
            <Text fx:id="text" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="200.13671875" BorderPane.alignment="CENTER" />
         </center>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </BorderPane>
   </children>
</StackPane>
